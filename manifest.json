{
  "name": "Timetrakt for Chrome",
  "version": "1.0",
  "description": "Timetrakt timer for Chrome",
  "manifest_version": 2,
  "background": {
    "scripts": ["bundled/axios.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Timetrakt"
  },
  "permissions": ["storage", "declarativeContent", "https://timetrakt.com/api/*"],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  }
}